{
  "name": "hey-babe-triplit",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pkill -f 'vite dev' || true && vite dev --port 3000",
    "dev:clean": "pkill -f 'vite dev' || true && sleep 1 && vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "ts:check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:test": "bun run src/server/db/test-connection.ts",
    "db:test:new": "bun run src/server/db/test-connection-new.ts",
    "db:setup": "bun run src/scripts/db-setup.ts",
    "db:reset": "bun run src/scripts/db-reset.ts",
    "db:seed": "bun run src/scripts/db-seed.ts",
    "db:help": "bun run src/scripts/db-help.ts",
    "config:validate": "bun run src/scripts/validate-config-consistency.ts",
    "demo:resources": "bun run src/demo/resource-management-demo.ts",
    "demo:auth": "bun run src/demo/auth-service-demo.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.4.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-router": "^1.131.26",
    "@tanstack/react-router-ssr-query": "^1.131.26",
    "@tanstack/react-start": "^1.131.26",
    "@tanstack/router-plugin": "^1.131.26",
    "better-auth": "^1.3.7",
    "drizzle-orm": "^0.44.4",
    "effect": "^3.17.7",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.6",
    "typescript-eslint": "^8.40.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tanstack/eslint-plugin-router": "^1.131.2",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router-devtools": "^1.131.26",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/bun": "^1.2.20",
    "@types/nodemailer": "^7.0.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "^0.31.4",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "web-vitals": "^4.2.4"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "esbuild",
    "unrs-resolver"
  ]
}
